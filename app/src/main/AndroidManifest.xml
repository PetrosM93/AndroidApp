<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="com.example.terrorizer.teopiotrpromitheuths">


    <application
        android:icon="@mipmap/ic_launcher"
        android:label="Stock Manager"
        android:roundIcon="@mipmap/background"
        android:supportsRtl="true"
        android:theme="@style/AppTheme"
        tools:ignore="GoogleAppIndexingWarning">
        <activity android:name=".MainActivity" />
        <activity android:name=".AddPelates" />
        <activity android:name=".NewOrder" />
        <activity android:name=".OrdersToday" />

        <receiver
            android:name=".AlarmReceiver"
            tools:ignore="ExportedReceiver">
            <intent-filter>
                <action android:name="singh.ajit.action.DISPLAY_NOTIFICATION" />
            </intent-filter>
        </receiver>

        <activity android:name=".loadingScreen">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".ShowPelates" />
        <activity android:name=".ViewPelatisProfile" />
        <activity android:name=".EditPelates" />
        <activity android:name=".AddItems" />
        <activity android:name=".EditItems" />
        <activity android:name=".ViewItemProfile" />
        <activity android:name=".orderbydate" />
        <activity android:name=".showItems"/>
    </application>

</manifest>